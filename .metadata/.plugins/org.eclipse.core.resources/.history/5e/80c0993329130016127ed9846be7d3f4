#include <iostream>
#include <string>
using namespace std;

int main(){
	string key = "cat";
	unsigned int i = 0;
	int result = 0;
	//search the nth prime number
	while (i <= key.length()) {
		result++;
		if (isPrime(result))
			i++;
	}
	return result;

	return 0;
}



