#include "QuadraticProbing.h"


QuadraticProbing :: QuadraticProbing(int a):OpenAddressing(a){}

int QuadraticProbing :: getIndexByhashing(int keyValue){
	int initValue = keyValue % 6;
	int resultValue;
	int i = 1;
	while(data[initValue] != EMPTY){
		resultValue = (initValue + (i * i)) % bucketSize;
		i++;
	}
	return resultValue;
}



