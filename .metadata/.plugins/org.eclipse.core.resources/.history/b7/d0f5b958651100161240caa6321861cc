#include <iostream>
#include <string>
using namespace std;




int main(){
	string a = "ca";
	int count = 0;
	for(string :: iterator iter = a.begin() ; iter != a.end() ; iter++){
		count += *iter;
		cout<<(int)*iter<<endl;
	}

	cout<<"count: "<<count<<endl;

	cout<<"haha: "<<a[7]<<endl;
	int result = 0 ,temp;
	int i = 0;
	const int hash_const  = 27;

	for(string::iterator it = a.begin(); it != a.end() ; it++){
		temp = i * hash_const * (*it);
		result += temp;
		i++;
	}
	cout<<"algo 2: "<<result;




	return 0;
}



