#include <iostream>
#include <string>
using namespace std;

bool isPrime(int n) {
	//determine whether it is a prime number or not
	int flag = 0;
	for (int i = 2; i <= n / 2; ++i) {
		if (n % i == 0) {
			flag = 1;
			break;
		}
	}
	if (flag == 0)
		return true;
	else
		return false;
}

int hashFunction(string& key) {
	unsigned int i = 0;
	int result = 0;
	//search the nth prime number
	while(i < key.length()){
		if(isPrime(result)){
			i++;
			result++;
		}
		result++;
	}
	return result;
}
int main() {
	string a = "cat";
	cout<<"result: "<<hashFunction(a)<<endl;
	return 0;
}

