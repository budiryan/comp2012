#include <iostream>
#include <string>
using namespace std;

int main() {
	string key = "cat";
	unsigned int i = 0;
	int result = 0, flag = 1;
	//search the nth prime number
	while (i <= key.length()) {
		result++;
		for (int i = 2; i <= result / 2; ++i) {
			if (result % i == 0) {
				flag = 1;
				break;
			}
		}
		if (flag == 0)
			i++;
		flag = 1;
	}
	cout<<"result: "<< result;

	return 0;
}

