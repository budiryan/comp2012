#include "QuadraticProbing.h"


int QuadraticProbing :: getIndexByhashing(int keyValue){
	int initValue = keyValue % 6;
	int i = 1;
	while(data[initValue] != EMPTY){
		initValue = (initValue + pow(i,2)) % bucketSize;
	}
	return initValue;
}



