#include "LinearProbing.h"


int LinearProbing :: getIndexByhashing(int keyValue){
	int initValue = keyValue % 6;
	while(data[initValue] != EMPTY){
		initValue = (initValue + 1) % bucketSize;
	}
	return initValue;
}


