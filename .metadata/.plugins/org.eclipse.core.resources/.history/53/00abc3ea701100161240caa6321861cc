#include <iostream>
#include <string>
using namespace std;

bool isPrime(int n) {
	//isprime?
	int flag = 0;
	for (int i = 2; i <= n / 2; ++i) {
		if (n % i == 0) {
			flag = 1;
			break;
		}
	}
	if (flag == 0)
		return true;
	else
		return false;
}

int main() {
	string key = "cat";
	int temp, i = 0, const_multiplier = 1, result = 0;
	const int hash_const = 37;
	if (!key.length())
		cout<<0;
	for (string::iterator it = key.begin(); it != key.end(); it++, i++) {
		for (int k = key.length() - 1; k > i; k--)
			const_multiplier *= hash_const;
		temp = const_multiplier * (*it);
		result += temp;
		const_multiplier = 1;
	}
	cout<<"result "<<result<<endl;
	return 0;
}

