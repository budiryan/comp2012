#include <iostream>
#include <string>
using namespace std;




int main(){
	string key = "ca";
	int length = key.length() , temp, i = 0, const_multiplier = 1, result = 0;
	const int hash_const = 37;
	if(!key.length())
		cout<<0;
	for(string::iterator it = key.begin(); it != key.end(); it++){
		for(int k = length - 1; k >= 0 ; k--)
			const_multiplier *= hash_const;
		temp = const_multiplier * (*it);
		result += temp;
		const_multiplier = 1;
	}
	cout<<"result: "<<result;


	return 0;
}



